	"scripts": {
		"clean": "rimraf dist",
		"build:webpack": "cross-env NODE_ENV=production webpack --display-error-details --config webpack.config.prod.js",
		"build": "npm run clean && npm run build:webpack",
		"dev": "cross-env NODE_ENV=development npm start",
		"start": "nodemon --exec babel-node --ignore src/ --ignore client/ --ignore components/ src/server/server.js",
		"prod": "cross-env NODE_ENV=production npm run start",
		"lint": "eslint src",
		"heroku-postbuild": "npm run build"
	},
